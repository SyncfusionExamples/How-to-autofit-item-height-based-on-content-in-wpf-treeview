    <Resource Include="Images\treeview_img1.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\treeview_mp3.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\treeview_pdf.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\treeview_png.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\treeview_ppt.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
