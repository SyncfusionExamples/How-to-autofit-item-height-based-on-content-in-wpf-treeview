<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>AutoFitContentDemo</AssemblyName>
    <RootNamespace>AutoFitContentDemo</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <Resource Include="App.ico" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\collapse.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\expand.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
    <Resource Include="Images\treeview_exe.png" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" />
